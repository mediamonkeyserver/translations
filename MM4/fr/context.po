#~ msgid "Search Tips:"
#~ msgstr "Astuces de Recherche:"

#~ msgid "+A : finds tracks with A* only"
#~ msgstr "+A : trouve les pistes avec A* seulement"

#~ msgid "\"A\" : finds tracks with A only"
#~ msgstr "\"A\" : trouve les pistes avec seulement A"

#~ msgid "A B : finds tracks with A AND B"
#~ msgstr "A B : trouve les pistes avec A ET B"

#~ msgid "A OR B : finds tracks with either A or B"
#~ msgstr "A OR B : trouve les pistes avec A OU B"

#~ msgid "A -B : find tracks with A and NOT B"
#~ msgstr "A -B : trouve les pistes avec A mais pas B"

#~ msgid "artist:A : finds tracks with Artist = A"
#~ msgstr "artist:A : trouve les pistes avec Artiste = A"

#~ msgid "year:X..Y : finds tracks in range X to Y"
#~ msgstr "year:X..Y : trouve les pistes dont l'année se situe entre X et Y"

#~ msgid "rating: X.. : finds tracks with X stars or more"
#~ msgstr ""
#~ "rating: X.. : trouve les pistes avec une notation de X étoiles ou plus"
